<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="wres" type="com.jitlogic.zico.client.widgets.WidgetResources"/>
    <g:HTMLPanel styleName="{wres.formCss.form}">
        <table>
            <tr>
                <td><g:Label>Host name:</g:Label></td>
                <td><g:TextBox ui:field="txtHostName"/></td>
            </tr>
            <tr>
                <td><g:Label>Host address:</g:Label></td>
                <td><g:TextBox ui:field="txtHostAddr"/></td>
            </tr>
            <tr>
                <td><g:Label>Host group:</g:Label></td>
                <td><g:TextBox ui:field="txtHostGroup"/></td>
            </tr>
            <tr>
                <td><g:Label>Host description:</g:Label></td>
                <td><g:TextBox ui:field="txtHostDesc"/></td>
            </tr>
            <tr>
                <td><g:Label>Host password:</g:Label></td>
                <td><g:TextBox ui:field="txtHostPass"/></td>
            </tr>
            <tr>
                <td><g:Label>Host password:</g:Label></td>
                <td><g:TextBox ui:field="txtMaxSize"/></td>
            </tr>
            <tr>
                <td><g:Button ui:field="btnOk">OK</g:Button></td>
                <td><g:Button ui:field="btnCancel">Cancel</g:Button></td>
            </tr>
        </table>
    </g:HTMLPanel>
</ui:UiBinder>


