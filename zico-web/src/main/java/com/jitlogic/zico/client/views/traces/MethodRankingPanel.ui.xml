<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:gc='urn:import:com.google.gwt.user.cellview.client'>
    <ui:style>
        .panel {
            margin-top: 2px;
        }
    </ui:style>
    <g:DockLayoutPanel unit="PX" addStyleNames="{style.panel}">
        <g:center>
            <gc:DataGrid ui:field="rankGrid"/>
        </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder>
